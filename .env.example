# Example environment configuration file for manage-agenda
# Copy this file to .env and fill in your actual values

# Google API Configuration
# Get your credentials from: https://console.cloud.google.com/
# GOOGLE_CALENDAR_CREDENTIALS_PATH=path/to/credentials.json
# GOOGLE_GMAIL_CREDENTIALS_PATH=path/to/credentials.json

# Gemini API Key
# Get it from: https://ai.google.dev/
# GEMINI_API_KEY=your_gemini_api_key_here

# Mistral API Key
# Get it from: https://mistral.ai/
# MISTRAL_API_KEY=your_mistral_api_key_here

# Ollama Configuration (if using local models)
# OLLAMA_HOST=http://localhost:11434
# OLLAMA_DEFAULT_MODEL=llama2

# Default timezone for events (IANA timezone name)
DEFAULT_TIMEZONE=Europe/Berlin

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=manage_agenda.log

# Email Configuration
DEFAULT_EMAIL_TAG=zAgenda
